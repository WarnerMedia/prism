    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "jest-date-mock": "^1.0.8",
    "jest-environment-jsdom": "^27.5.1",
    "jest-extended": "^3.0.0",
    "jest-when": "^3.1.0",
    "lint-staged": "^10.2.13",
    "mock-local-storage": "^1.1.17",
    "npm-run-all": "^4.1.5",
    "patch-package": "^6.4.7",
    "prettier": "^2.1.0",
    "sinon": "^15.1.0",
    "ts-jest": "^27.1",
    "ts-mockito": "^2.6.1",
    "tsup": "^6.5.0",
    "typescript": "^4.3.2",
    "wait-on": "^5.3.0"
